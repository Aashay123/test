#include<stdio.h>
#include<stdlib.h>

void display(int *p,int n)
{

for(int i=0;i<n;i++)
{
printf("%d\n",*(p+i));
}

}

//nice program

int main()
{
int n;
printf("enter n : \n");
scanf("%d",&n);

int *p=(int *)malloc((sizeof(int))*n);

printf("\nenter elements : ");
for(int i=0;i<n;i++)
{
scanf("%d",p+i);
}
display(p,n);

//make all zero
for(int i=0;i<n;i++)
{
}

return 0;
}
